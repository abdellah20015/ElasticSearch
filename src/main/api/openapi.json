{
  "openapi": "3.0.0",
  "info": {
    "title": "CSV to Elasticsearch API",
    "version": "1.0.0",
    "description": "API for uploading large CSV files and importing them to Elasticsearch"
  },
  "paths": {
    "/data/file/import": {
      "post": {
        "operationId": "uploadCsvToElasticsearch",
        "summary": "Upload CSV file and import to Elasticsearch",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "CSV file to upload"
                  },
                  "indexName": {
                    "type": "string",
                    "description": "Elasticsearch index name"
                  },
                  "batchSize": {
                    "type": "integer",
                    "description": "Number of records to process in each batch",
                    "default": 5000
                  }
                },
                "required": ["file", "indexName"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "CSV file uploaded and processing started",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": ["success", "error"]
                    },
                    "message": {
                      "type": "string"
                    },
                    "jobId": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/data/search": {
      "post": {
        "operationId": "searchElasticsearch",
        "summary": "Search data in Elasticsearch index 'elastic_search'",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "query": {
                    "type": "string",
                    "description": "Search query string"
                  },
                  "page": {
                    "type": "integer",
                    "default": 1,
                    "minimum": 1,
                    "description": "Page number for pagination"
                  },
                  "size": {
                    "type": "integer",
                    "default": 10,
                    "minimum": 1,
                    "maximum": 100,
                    "description": "Number of results per page"
                  }
                },
                "required": ["query"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Search results from elastic_search index",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": ["success"]
                    },
                    "results": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "total": {
                      "type": "integer",
                      "description": "Total number of matching results"
                    },
                    "count": {
                      "type": "integer",
                      "description": "Number of results in this response"
                    },
                    "page": {
                      "type": "integer",
                      "description": "Current page number"
                    },
                    "size": {
                      "type": "integer",
                      "description": "Results per page"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": ["error"]
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
